<h1>University of Panic</h1>
<hr />
<h2>Students:</h2>
@if (Model.Count != 0)
{
  <Ol>
    @foreach (var student in Model)
    {
      <li><a href='/students/@student.GetId()'>Id: @student.GetId()Name: @student.GetName()Enrollment Date: @student.GetEnrollmentDate()</a></li>
    }

  </Ol>
}
<h4><a href="/students/new">Add new student</a></h4>
<h4><a href="/">Return home</a></h4>
